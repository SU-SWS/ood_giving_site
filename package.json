{
  "name": "giving-to-stanford",
  "private": true,
  "description": "Giving to Stanford",
  "version": "2.2.0",
  "author": "Stanford University Office of Development",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "acorn": "^8.0.4",
    "algoliasearch": "^4.8.3",
    "autoprefixer": "^9.8.6",
    "bourbon": "^5.1.0",
    "decanter": "^6.2.2",
    "gatsby": "^2.24.91",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-algolia": "^0.16.2",
    "gatsby-plugin-anchor-links": "^1.2.0",
    "gatsby-plugin-google-tagmanager": "^2.7.0",
    "gatsby-plugin-manifest": "^2.4.33",
    "gatsby-plugin-offline": "^3.2.30",
    "gatsby-plugin-postcss": "^2.3.13",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sharp": "^2.6.38",
    "gatsby-plugin-sitemap": "^2.4.14",
    "gatsby-source-filesystem": "^2.3.32",
    "gatsby-source-storyblok": "^1.1.1",
    "gatsby-transformer-sharp": "^2.5.16",
    "netlify-lambda": "^2.0.1",
    "netlify-plugin-submit-sitemap": "^0.2.3",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^16.13.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.8.2",
    "react-loader-spinner": "^3.1.14",
    "react-player": "^2.8.2",
    "react-refresh": "^0.8.3",
    "sanitize-html": "^1.27.5",
    "storyblok-react": "^0.1.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "prettier": "2.0.5",
    "release-it": "^14.3.0"
  },
  "keywords": [
    "Stanford"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "release:bump": "npm version prerelease",
    "release:bump-patch": "npm version patch",
    "release:bump-minor": "npm version minor",
    "release:bump-major": "npm version major",
    "release:changelog": "auto-changelog -p --tag-prefix && git add CHANGELOG.md",
    "release": "release-it --ci",
    "release:dev": "release-it --ci --github.preRelease --preReleaseId=dev --npm.tag=dev -- minor",
    "release:manual": "release-it"
  },
  "release-it": {
    "git": {
      "changelog": "auto-changelog --unreleased-only --stdout",
      "requireUpstream": false
    },
    "github": {
      "release": true,
      "releaseNotes": "auto-changelog --unreleased-only --stdout"
    },
    "npm": {
      "publish": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SU-SWS/ood_giving_site"
  },
  "bugs": {
    "url": "https://github.com/SU-SWS/ood_giving_site/issues"
  }
}
